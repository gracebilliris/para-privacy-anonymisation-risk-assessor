{
    "name": "agentic-privacy-monitor",
    "version": "1.0.0",
    "description": "Agentic system for privacy risk assessment using anonymisation validator.",
    "main": "dist/api/server.js",
    "scripts": {
        "build": "tsc",
        "start": "npm run build && node dist/api/server.js",
        "agents": "npm-run-all --parallel agent-orchestrator agent-validator agent-scanner agent-summariser",
        "agent-orchestrator": "python3.11 -m uvicorn privacy_validator.orchestrator_service:app --port 8000",
        "agent-validator": "python3.11 -m uvicorn privacy_validator.validator_service:app --port 8001",
        "agent-scanner": "python3.11 -m uvicorn privacy_validator.scanner_service:app --port 8002",
        "agent-summariser": "python3.11 -m uvicorn privacy_validator.summariser_service:app --port 8003",
        "dev": "npm-run-all --parallel start agents"
    },
    "author": "gracebilliris",
    "license": "MIT",
    "dependencies": {
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "express": "^4.21.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.23",
        "@types/node": "^24.3.1",
        "typescript": "^5.0.0",
        "npm-run-all": "^4.1.5"
    }
}
